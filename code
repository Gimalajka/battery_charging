import csv
import matplotlib.pyplot as plt

with open('Busmaster_Cy1_.csv', 'r') as f:
    reader = csv.reader(f)
    header = next(reader)

    data = {}
    for row in reader:                              # Создание словаря, где ключ - Execution_Time, значение - Current
        key = row[1]
        values = [float(x) for x in row[24:25]]
        data[key] = values

    for val in data.values():                  # поиск и измерение участков, где Current = 0
        if val == 0
        print(data[val])
        if val != 0:
            print(data[val])



with open('Busmaster_Cy1_.csv', 'r') as f:      # заметки
    reader = csv.reader(f)
    next(reader)
    data = list(reader)
    time = (float(row[1]) for row in data)
    current = (float(row[24]) for row in data)
    data_dict = {"time", "current"}

x_data = [float(row[1]) for row in data]
y_data = [float(row[24]) for row in data]

plt.plot(x_data, y_data)
plt.show()

